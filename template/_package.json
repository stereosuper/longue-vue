{
    "name": "<%= name %>",
    "version": "1.0.0",
    "description": "<%= description %>",
    "author": "<%= author %>",
    "private": true,
    "scripts": {
        "dev": "cross-env HOST=0.0.0.0 PORT=3000 nuxt",
        "build": "nuxt build",
        "build:analyze": "nuxt build --analyze",
        "build:modern:analyze": "nuxt build --modern=server --analyze",
        "build:modern": "nuxt build --modern=server",
        "start": "nuxt start",
        "start:modern": "nuxt start --modern=server",
        "generate": "nuxt generate",
        "generate:modern": "nuxt generate --modern",
        "generate:dev": "cross-env HOST=0.0.0.0 PORT=3000 nuxt generate",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
        "": ""
    },
    "dependencies": {
        <%_ if (stereorepo.burger) { _%>
        "@stereorepo/burger": "^3.0.0",
        <%_ } _%>
        "@stereorepo/sac": "^3.0.0",
        "fastdom": "^1.0.9",
        <%_ if (packages.gsap) { _%>
        "gsap": "^3.0.0",
        <%_ } _%>
        "nuxt": "^2.0.0",
        "sharp": "^0.24.0"
        "": ""
    },
    "devDependencies": {
        "@nuxtjs/dotenv": "^1.4.0",
        <%_ if (features.pwa) { _%>
        "@nuxtjs/pwa": "^3.0.0-0",
        <%_ } _%>
        "@nuxtjs/robots": "^2.4.0",
        "@nuxtjs/sitemap": "^2.0.1",
        "@nuxtjs/style-resources": "^1.0.0",
        <%_ if (cms === 'dato' || cms === 'prismic') { _%>
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        <%_ } _%>
        <%_ if (cms === 'dato') { _%>
        "apollo-link-http": "^1.5.15",
        <%_ } else if (cms === 'prismic') { _%>
        "apollo-link-prismic": "^1.0.2",
        <%_ } _%>
        "babel-eslint": "^10.0.3",
        "cross-env": "^7.0.0",
        "dotenv": "^8.1.0",
        "eslint": "^6.3.0",
        "eslint-config-prettier": "^6.2.0",
        "eslint-loader": "^3.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^6.1.2",
        <%_ if (cms === 'dato' || cms === 'prismic') { _%>
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.1",
        "node-fetch": "^2.6.0",
        <%_ } _%>
        "node-sass": "^4.12.0",
        "nuxt-i18n": "^6.1.3",
        "prettier": "^1.18.2",
        "sass-loader": "^8.0.0",
        "": ""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stereosuper/longue-vue"
    },
    "keywords": [
        "vue.js",
        "nuxt.js"
    ],
    "license": "ISC"
}
